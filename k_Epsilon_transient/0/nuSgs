    {
        type           zeroGradient;
        
    }
     wall3
    {
        type            zeroGradient;
        
    }
     wall4
    {
        type            zeroGradient;
       
    }
     wall5
    {
        type           zeroGradient;
        
    }
    
    frontwall
    {
